<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<GUI name="easyDevControlsFields">
    <GuiElement type="empty" profile="devControlsFrame">
        <GuiElement type="empty" profile="devControlsHeaderPanel">
            <GuiElement type="text" profile="ingameMenuFrameHeaderText" id="frameHeaderText" text="$l10n_easyDevControls_fieldsHeader"/>
            <GuiElement type="button" profile="devControlsInfo" id="buttonShowInfo" onClick="onClickShowInfo"/>
        </GuiElement>

        <GuiElement type="empty" profile="devControlsContainer" id="container">
            <GuiElement type="boxLayout" profile="devControlsLayout" id="boxLayout">
                <!-- Column 1 -->
                <GuiElement type="button" profile="devControlsConfirm" name="setFruit" id="buttonFieldSetFruit" onClick="onClickFieldSet">
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_fieldSetFruitTitle" />
                </GuiElement>

                <GuiElement type="button" profile="devControlsConfirm" name="setGround" id="buttonFieldSetGround" onClick="onClickFieldSet">
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_fieldSetGroundTitle" />
                </GuiElement>

                <GuiElement type="multiTextOption" profile="devControlsMultiTextOption" id="checkedVineSetStateFruitType" onClick="onClickVineSetStateFruitType">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_vineSetStateTitle"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOptionClose" id="checkedVineSetStateGrowthState" onClick="onClickVineSetStateGrowthState">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="button" profile="devControlsConfirmClose" id="buttonConfirmVineSetState" onClick="onClickConfirmVineSetState"/>

                <GuiElement type="button" profile="devControlsConfirm" margin="44px 24px 44px 210px" debugMargin="true" id="buttonVineUpdateVisuals" onClick="onClickVineUpdateVisuals">
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_vineUpdateVisualsTitle" />
                </GuiElement>

                <!-- Column 2 -->
                <GuiElement type="bitmap" profile="devControlsTextDisplayBg">
                    <GuiElement type="text" profile="devControlsTextDisplayText" text="$l10n_easyDevControls_noField" id="textDisplayWeeds"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_setting_weedsEnabled"/>
                </GuiElement>
                <!-- <GuiElement type="empty" profile="devControlsAddRemoveButtonsClose"> -->
                    <GuiElement type="button" profile="devControlsAddRemoveButtonsLeft" name="removeWeeds" id="buttonRemoveWeeds" onClick="onClickRemove" />
                    <GuiElement type="button" profile="devControlsAddRemoveButtonsRight" name="addWeeds" id="buttonAddWeeds" onClick="onClickAdd" />
                <!-- </GuiElement> -->

                <GuiElement type="bitmap" profile="devControlsTextDisplayBg">
                    <GuiElement type="text" profile="devControlsTextDisplayText" text="$l10n_easyDevControls_noField" id="textDisplayStones"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_setting_stonesEnabled"/>
                </GuiElement>
                <!-- <GuiElement type="empty" profile="devControlsAddRemoveButtonsClose"> -->
                    <GuiElement type="button" profile="devControlsAddRemoveButtonsLeft" name="removeStones" id="buttonRemoveStones" onClick="onClickRemove"/>
                    <GuiElement type="button" profile="devControlsAddRemoveButtonsRight" name="addStones" id="buttonAddStones" onClick="onClickAdd" />
                <!-- </GuiElement> -->

                <GuiElement type="button" profile="devControlsConfirm" id="buttonConfirmAdvanceGrowth" onClick="onClickConfirmAdvanceGrowth">
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_advanceGrowthTitle" />
                </GuiElement>

                <GuiElement type="multiTextOption" profile="devControlsMultiTextOption" id="multiGrowthPeriod" texts="$l10n_ui_month1|$l10n_ui_month2|$l10n_ui_month3|$l10n_ui_month4|$l10n_ui_month5|$l10n_ui_month6|$l10n_ui_month7|$l10n_ui_month8|$l10n_ui_month9|$l10n_ui_month10|$l10n_ui_month11|$l10n_ui_month12">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_setGrowthPeriodTitle"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="button" profile="devControlsConfirmClose" margin="44px -18px 44px 150px" debugMargin="true" id="buttonConfirmGrowthPeriod" onClick="onClickConfirmGrowthPeriod"/>

                <!-- Column 3 -->
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOption" id="multiSetFarmlandOwnerSortBy" onClick="onClickSetFarmlandOwnerSortBy">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="text" profile="multiTextOptionTitle" text="$l10n_easyDevControls_setFarmlandOwnerTitle"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOptionClose" id="multiSetFarmlandFarmId" onClick="onClickSetFarmlandFarmId">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOptionClose" id="multiSetFarmlandOwnerIndex" onClick="onClickSetFarmlandOwnerIndex">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="button" profile="devControlsConfirmClose" id="buttonConfirmSetFarmlandOwner" onClick="onClickConfrimSetFarmlandOwner"/>

                <GuiElement type="bitmap" profile="devControlsTextDisplayBg">
                    <GuiElement type="text" profile="devControlsTextDisplayText" id="textRefreshFieldOverlay"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_refreshFieldOverlayTitle"/>
                </GuiElement>
                <GuiElement type="button" profile="devControlsRefresh" text="$l10n_button_refresh" margin="44px -18px 44px 290px" debugMargin="true" id="buttonRefreshFieldOverlay" onClick="onClickRefreshFieldOverlay"/>

                <!-- Column 4 -->
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOption" id="multiDebugFieldStatusRange" onClick="onClickDebugFieldStatusRange">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_debugFieldStatusTitle"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="checkedOption" profile="devControlsMultiTextOptionClose" id="checkedDebugFieldStatus" onClick="onClickDebugFieldStatus" >
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>

                <GuiElement type="checkedOption" profile="devControlsMultiTextOption" id="checkedDebugVineSystem" onClick="onClickDebugVineSystem" >
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_vineSystemDebugTitle"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>

                <GuiElement type="checkedOption" profile="devControlsMultiTextOption" id="checkedDebugStoneSystem" onClick="onClickDebugStoneSystem" >
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_stoneSystemDebugTitle"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
            </GuiElement>
        </GuiElement>

        <GuiElement type="bitmap" profile="devControlsInfoRowBg">
            <GuiElement type="bitmap" profile="devControlsInfoRowIcon" id="infoBoxIcon"/>
            <GuiElement type="text" profile="devControlsInfoRowText" id="infoBoxText" text=""/>
        </GuiElement>
    </GuiElement>
</GUI>
