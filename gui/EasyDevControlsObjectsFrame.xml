                <GuiElement type="multiTextOption" profile="devControlsMultiTextOptionClose" id="multiClearTipAnywhereArea" onClick="onClickClearTipAnywhereArea">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="button" profile="devControlsConfirmClose" margin="44px -18px 44px 180px" debugMargin="true" id="buttonConfirmClearTipAnywhere" onClick="onClickConfirmClearTipAnywhere"/>

