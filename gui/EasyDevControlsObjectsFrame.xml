                </GuiElement>
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOptionClose" id="multiTipToTriggerState" onClick="onClickTipToTriggerState">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="textInput" profile="devControlsTextInputClose" id="textInputTipToTriggerAmount" onEnterPressed="onTipToTriggerAmountEnterPressed" onEscPressed="onTextInputEscPressed" onTextChanged="onTextInputTextChanged" enterWhenClickOutside="true"/>
                <GuiElement type="button" profile="devControlsConfirmClose" id="buttonConfirmTipToTrigger" onClick="onClickConfirmTipToTrigger"/>

                <!-- Column 3 -->
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOption" id="multiTipAnywhereFillType">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
