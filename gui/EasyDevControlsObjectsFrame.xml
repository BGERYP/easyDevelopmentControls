        <GuiElement type="empty" profile="devControlsContainer" id="container">
            <GuiElement type="boxLayout" profile="devControlsLayout" id="boxLayout">
                <!-- Column 1 -->
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOption" id="multiBaleType" onClick="onClickBaleType">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_addBaleTitle"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOptionClose" id="multiBaleSize" onClick="onClickBaleSize">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOptionClose" id="multiBaleFillType" onClick="onClickBaleFillType">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="multiTextOption" profile="devControlsMultiTextOptionClose" id="multiBaleWrapState" onClick="onClickBaleWrapState">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="button" profile="devControlsConfirmClose" id="buttonConfirmBale" onClick="onClickConfirmBale"/>

                <GuiElement type="button" profile="devControlsConfirm" id="buttonBaleList" onClick="onClickShowBaleList">
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_availableBaleTypesTitle" />
                </GuiElement>

                <GuiElement type="button" profile="devControlsConfirm" id="buttonBalesFermenting" onClick="onClickShowBalesFermenting">
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_fermentingBalesTitle" />
                </GuiElement>

                <GuiElement type="checkedOption" profile="devControlsMultiTextOption" name="TYPE_BALE" id="checkedShowBaleLocations" onClick="onClickShowLocations">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_showBaleLocationsTitle"/>
