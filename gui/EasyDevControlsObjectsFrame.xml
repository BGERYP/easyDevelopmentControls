                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>
                <GuiElement type="button" profile="devControlsConfirmClose" id="buttonConfirmPallet" onClick="onClickConfirmPallet"/>

                <GuiElement type="multiTextOption" profile="devControlsMultiTextOption" id="multiLogType" onClick="onClickLogType">
                    <GuiElement type="button" profile="devControlsMultiTextOptionLeft"/>
                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_addLogTitle"/>
