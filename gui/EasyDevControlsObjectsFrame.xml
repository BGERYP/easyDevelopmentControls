                    <GuiElement type="button" profile="devControlsMultiTextOptionRight"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionText"/>
                    <GuiElement type="text" profile="devControlsMultiTextOptionTitle" text="$l10n_easyDevControls_showTipCollisionsTitle"/>
                    <GuiElement type="bitmap" profile="devControlsMultiTextOptionBg"/>
                </GuiElement>

                <GuiElement type="button" profile="devControlsConfirm" name="BALES" id="buttonConfirmRemoveAllBales" onClick="onClickConfirmRemoveAll">
